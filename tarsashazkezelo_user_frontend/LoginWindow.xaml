<Window x:Class="tarsashazkezelo_user_frontend.LogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tarsashazkezelo_user_frontend"
        mc:Ignorable="d"
        Title="LogIn" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="100,159,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Bejelentkezés" HorizontalAlignment="Left" Margin="100,29,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="100,102,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label1" Content="Lakás szám" HorizontalAlignment="Left" Margin="10,98,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Jelszó" HorizontalAlignment="Left" Margin="38,159,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Bejelentkezés" HorizontalAlignment="Left" Margin="100,224,0,0" VerticalAlignment="Top" Width="81"/>

    </Grid>
</Window>
